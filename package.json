{
  "name": "quasarboilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for hybrid applications using Node.js for backend, RethinkDB for database, and Quasar (Vue.js framework) for frontend/app Edit",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -w src --exec npm run devcheck",
    "start": "node dist/server.js",
    "seed": "babel-node src/seed.js",
    "devcheck": "eslint --ext .js src && babel-node src/server.js || true",
    "lint": "eslint --ext .js src || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielsclimaco/QuasarBoilerplate.git"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "nodemon": "^1.11.0",
    "socket.io": "^1.7.3",
    "thinky": "^2.3.8",
    "uuid": "^3.0.1",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsonwebtoken": "^7.4.0",
    "lodash": "^4.17.4"
  }
}
